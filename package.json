{
  "name": "norplas-react-common",
  "version": "1.1.6",
  "description": "Common React modules for use within norplas.",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "style": "dist/styles.css",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm-run-all -p build:clean  build:ts",
    "build:ts": "npx rollup -c",
    "build:clean": "rm -rf ./dist/*",
    "test": "jest",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "repository": {
    "type": "git",
    "url": "Norplas@vs-ssh.visualstudio.com:v3/Norplas/react-common/react-common"
  },
  "bugs": {
    "url": "https://norplas.visualstudio.com/react-common/_workitems/recentlyupdated/"
  },
  "homepage": "https://norplas.visualstudio.com/react-common",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "rollup",
    "react",
    "norplas",
    "react component",
    "rollup react",
    "component"
  ],
  "author": "Charles.Berman@Magna.com",
  "license": "MIT",
  "dependencies": {
    "node-fetch": "^2.6.0",
    "notistack": "^1.0.3"
  },
  "peerDependencies": {
    "@material-ui/core": "4.0.0",
    "prop-types": "^15.7.2",
    "react": "^16.14.0",
    "socket.io-client": "2.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@storybook/addon-actions": "^6.1.18",
    "@storybook/addon-essentials": "^6.1.18",
    "@storybook/addon-links": "^6.1.18",
    "@storybook/react": "^6.1.18",
    "@testing-library/react": "^10.0.4",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.22",
    "@types/node-fetch": "^2.5.8",
    "@types/react": "^17.0.0",
    "@types/socket.io-client": "^1.4.35",
    "babel-loader": "^8.1.0",
    "jest": "^25.5.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-is": "^16.13.1",
    "rollup": "^2.2.0",
    "rollup-plugin-typescript2": "^0.27.0",
    "ts-jest": "^25.4.0",
    "typescript": "^3.8.3"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  },
  "engines": {
    "node": ">=10.0.0"
  }
}
